ce.parentElement.classList.add(classToApply);\n\n        setTimeout(() => {\n            selectedChoice.parentElement.classList.remove(classToApply)\n            this.getNewQuestion()\n        }, 300)\n    }\n\n    incrementScore = num => {\n        this.state.score += num;\n    }\n\n    getTimer = () => {\n        var totalTime = 60;\n        setInterval(() => {\n            totalTime--\n            this.setState({\n                timeLeft: totalTime\n            })\n            if (this.state.timeLeft < 1) {\n                this.stopGame()\n            }\n        }, 1000)\n    }\n\n    render() {\n\n\n        return (\n            <div>\n            <Navbar />\n            <div className=\"gk\">\n                <div className=\"quiz-gk-pic\">\n                <div className=\"game-gk-container\">\n                    <div id=\"quiz-game-gk\" className=\"justify-center flex-column\">\n                        <div id=\"hud\">\n                            <div className=\"hud-item\">\n                                <p id=\"progressText\" className=\"hud-prefix\">\n                                    Time Left: {this.state.timeLeft}s\n                                </p>\n                            </div>\n                            <div className=\"hud-item\">\n                                <p className=\"hud-prefix\">\n                                    Score\n                                </p>\n                                <h1 className=\"hud-main-text\" id=\"score\">\n                                    {this.state.score}\n                                </h1>\n                            </div>\n                        </div>\n                        <div className=\"question-flex\">\n                        <h1 className=\"\" id=\"question\">{this.state.currentQuestion.question}</h1>\n                        <div className=\"choice-container\">\n                            <p className=\"choice-prefix\">A</p>\n                            <p classNa